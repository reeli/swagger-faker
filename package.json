{
  "name": "swagger-faker",
  "version": "3.1.1",
  "description": "Generate mocker services automatically through Swagger or OpenAPI",
  "bin": {
    "swagger-faker": "bin/cli.js"
  },
  "main": "dist/lib/index.js",
  "module": "dist/module/index.js",
  "types": "src/index.ts",
  "files": [
    "lib",
    "module",
    "src",
    "README.md"
  ],
  "scripts": {
    "start": "ts-node src/cli/index.ts gen && ts-node src/cli/index.ts run",
    "mock": "npx json-server --watch mock-server/db.js --middlewares mock-server/middlewares/*.js",
    "test": "jest",
    "build": "rm -rf .output/ && tsc",
    "release": "rm -rf dist/ && rollup -c rollup.config.ts",
    "prepare": "npm run release && npm test",
    "copy": "cp docs/index.html docs/404.html",
    "doc": "typedoc && pnpm copy"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:reeli/swagger-faker.git"
  },
  "author": "reeli",
  "license": "MIT",
  "bugs": {
    "url": "git@github.com:reeli/swagger-faker.git"
  },
  "homepage": "git@github.com:reeli/swagger-faker.git",
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/parser": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@changesets/cli": "^2.17.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-typescript": "^8.3.0",
    "@types/axios": "0.14.0",
    "@types/faker": "^5.5.9",
    "@types/inquirer": "^8.1.3",
    "@types/jest": "27.0.2",
    "@types/json-server": "^0.14.4",
    "@types/lodash": "4.14.176",
    "@types/node": "16.11.6",
    "@types/prettier": "2.4.1",
    "@types/rollup": "0.54.x",
    "@types/swagger-schema-official": "^2.0.22",
    "inquirer": "^8.2.0",
    "jest": "^27.3.1",
    "rollup": "^2.59.0",
    "ts-node": "10.4.0",
    "typedoc": "^0.22.10",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "commander": "^8.3.x",
    "faker": "^5.5.3",
    "json-server": "^0.17.0",
    "lodash": "^4.17.21",
    "path-to-regexp": "^6.2.0",
    "prettier": "^2.4.1",
    "swagger2openapi": "^7.0.8"
  }
}
