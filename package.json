{
  "name": "swagger-faker",
  "version": "1.0.0",
  "description": "Generate fake data from swagger",
  "main": "lib/index.js",
  "module": "module/index.js",
  "types": "src/index.ts",
  "files": [
    "lib",
    "module",
    "src",
    "README.md"
  ],
  "scripts": {
    "start": "ts-node examples/command.ts",
    "test": "jest",
    "build": "rm -rf dist/ && tsc",
    "release": "npm run build:es5 && npm run build:es6",
    "build:es5": "rm -rf lib && tsc --build ./tsconfig.build.es5.json",
    "build:es6": "rm -rf module && tsc --build ./tsconfig.build.es6.json",
    "prepare": "npm run release && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:reeli/swagger-faker.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "git@github.com:reeli/swagger-faker.git"
  },
  "homepage": "git@github.com:reeli/swagger-faker.git",
  "devDependencies": {
    "@babel/core": "7.5.5",
    "@babel/parser": "7.5.5",
    "@babel/preset-env": "7.5.5",
    "@babel/preset-typescript": "7.3.3",
    "@babel/traverse": "7.5.5",
    "@babel/types": "7.5.5",
    "@ts-tool/ts-codegen": "0.7.1",
    "@types/axios": "0.14.0",
    "@types/faker": "4.1.5",
    "@types/jest": "24.0.15",
    "@types/lodash": "4.14.136",
    "@types/node": "12.6.8",
    "@types/path-to-regexp": "1.7.0",
    "@types/prettier": "1.18.0",
    "@types/swagger-schema-official": "2.0.18",
    "axios": "0.19.0",
    "babel-jest": "24.8.0",
    "commander": "2.20.0",
    "faker": "4.1.0",
    "jest": "24.8.0",
    "path-to-regexp": "3.1.0",
    "prettier": "1.18.2",
    "ts-node": "8.3.0",
    "typescript": "3.5.3",
    "lodash": "4.17.15"
  },
  "jest": {
    "testEnvironment": "node",
    "coverageDirectory": "coverage",
    "testRegex": ".*/__tests__/.+\\.(generator|test|spec)\\.(ts|tsx)$",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/__types__/**",
      "!src/__tests__/**"
    ]
  }
}
