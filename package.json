{
  "name": "swagger-faker",
  "version": "2.0.0",
  "description": "Generate fake data from swagger",
  "main": "lib/index.js",
  "module": "module/index.js",
  "types": "src/index.ts",
  "files": [
    "lib",
    "module",
    "src",
    "README.md"
  ],
  "scripts": {
    "start": "ts-node json-server/configServer.ts",
    "mock": "npx json-server --watch mock-server/db.js --routes mock-server/routes.json",
    "test": "jest",
    "build": "rm -rf dist/ && tsc",
    "release": "rm -rf lib/ && rm -rf module/ && rollup -c rollup.config.ts",
    "prepare": "npm run release && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:reeli/swagger-faker.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "git@github.com:reeli/swagger-faker.git"
  },
  "homepage": "git@github.com:reeli/swagger-faker.git",
  "devDependencies": {
    "@babel/core": "7.6.0",
    "@babel/parser": "7.6.0",
    "@babel/preset-env": "7.6.0",
    "@babel/preset-typescript": "7.6.0",
    "@babel/traverse": "7.6.0",
    "@babel/types": "7.6.1",
    "@types/axios": "0.14.0",
    "@types/faker": "^5.5.7",
    "@types/jest": "24.0.18",
    "@types/lodash": "4.14.138",
    "@types/node": "12.7.5",
    "@types/path-to-regexp": "1.7.0",
    "@types/prettier": "1.18.2",
    "@types/rollup": "0.54.x",
    "@types/swagger-schema-official": "^2.0.22",
    "axios": "0.19.0",
    "babel-jest": "24.9.0",
    "babel-plugin-pure-calls-annotation": "0.2.x",
    "commander": "3.0.1",
    "jest": "^26.6.3",
    "lodash": "4.17.15",
    "path-to-regexp": "3.1.0",
    "prettier": "^2.2.1",
    "rollup": "1.21.x",
    "rollup-plugin-babel": "4.3.x",
    "rollup-plugin-typescript": "1.0.x",
    "ts-node": "8.4.1",
    "tslib": "1.10.0",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "faker": "^5.5.3",
    "swagger2openapi": "^7.0.5"
  },
  "peerDependencies": {
    "lodash": ">=4.17.0",
    "tslib": ">=1.9.0"
  },
  "jest": {
    "testEnvironment": "node",
    "coverageDirectory": "coverage",
    "testRegex": ".*/__tests__/.+\\.(generator|test|spec)\\.(ts|tsx)$",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/__types__/**",
      "!src/__tests__/**"
    ]
  }
}
